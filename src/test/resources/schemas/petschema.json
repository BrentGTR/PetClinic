{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "birthDate": {
      "type": "string",
      "format": "date"
    },
    "type": {
      "type": "string"
    },
    "ownerId": {
      "type": "integer"
    }
  },
  "required": ["id", "name", "birthDate", "type", "ownerId"]
}
